<Page x:Class="Automedon.Contacts"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Automedon"
      mc:Ignorable="d" 
      d:DesignHeight="340" d:DesignWidth="675"
      Title="Contacts" Background="LightGray">

    <Grid>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="34" Margin="207,93,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="34" Margin="207,47,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
        <TextBox x:Name="textBox3" HorizontalAlignment="Left" Height="34" Margin="207,141,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
        <TextBox x:Name="textBox4" HorizontalAlignment="Left" Height="34" Margin="207,245,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
        <TextBox x:Name="textBox5" HorizontalAlignment="Left" Height="34" Margin="207,191,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="203"/>
        <Label x:Name="label1" Content="Имя:" HorizontalAlignment="Left" Margin="104,93,0,0" VerticalAlignment="Top" Width="36" Height="34"/>
        <Label x:Name="label2" Content="Фамилия:" HorizontalAlignment="Left" Margin="104,47,0,0" VerticalAlignment="Top" Width="63" Height="34"/>
